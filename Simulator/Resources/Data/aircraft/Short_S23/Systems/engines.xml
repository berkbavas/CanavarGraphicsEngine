        <product>
         <value>-1.0</value>
         <property>fcs/boost/control-range-norm</property>
        </product>
       </gt>
       <gt>
        <property>fcs/boost/throttle-pos-norm[0]</property>
        <value>0.0</value>
       </gt>
      </product>
     </sum>
    </product>
